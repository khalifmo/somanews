#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/vercel@41.7.0/node_modules/vercel/dist/node_modules:/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/vercel@41.7.0/node_modules/vercel/node_modules:/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/vercel@41.7.0/node_modules:/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/vercel@41.7.0/node_modules/vercel/dist/node_modules:/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/vercel@41.7.0/node_modules/vercel/node_modules:/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/vercel@41.7.0/node_modules:/Users/mohamedkhalif/Documents/somanews/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/vc.js" "$@"
else
  exec node  "$basedir/../../dist/vc.js" "$@"
fi
